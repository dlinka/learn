<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--
    output:
    bean2 init...
    bean1 init...

    <bean id="bean2" class="com.cr.leftCycle.demo1.Bean2"></bean>
    <bean id="bean1" class="com.cr.leftCycle.demo1.Bean1"></bean>
    -->

    <!--
    output:
    Bean2 init...
    Bean1 init...
    set bean1...

    <bean id="bean2" class="com.cr.leftCycle.demo2.Bean2">
        <property name="bean1" ref="bean1"></property>
    </bean>
    <bean id="bean1" class="com.cr.leftCycle.demo2.Bean1"></bean>
    -->

    <!--
    output:
    Bean2 init...
    Bean1 init...
    set bean1...
    Bean3 init...

    <bean id="bean2" class="com.cr.leftCycle.demo3.Bean2">
        <property name="bean1" ref="bean1"></property>
    </bean>
    <bean id="bean3" class="com.cr.leftCycle.demo3.Bean3"></bean>
    <bean id="bean1" class="com.cr.leftCycle.demo3.Bean1"></bean>
    -->

    <!--
    output
    Bean2 init...
    Bean1 init...
    set bean1...
    afterPropertiesSet...
    initMethod...
    Bean3 init...

    <bean id="bean2" class="com.cr.leftCycle.demo4.Bean2" init-method="initMethod">
        <property name="bean1" ref="bean1"></property>
    </bean>
    <bean id="bean1" class="com.cr.leftCycle.demo4.Bean1"></bean>
    <bean id="bean3" class="com.cr.leftCycle.demo4.Bean3"></bean>
    -->

    <!--
    output:
    BeanFactoryPostProcessor2 postProcessBeanFactory...
    BeanFactoryPostProcessor1 postProcessBeanFactory...
    Bean2 init...
    Bean1 init...
    BeanPostProcessor2 bean1 postProcessBeforeInitialization...
    BeanPostProcessor1 bean1 postProcessBeforeInitialization...
    Bean1 afterPropertiesSet...
    Bean1 initMethod...
    BeanPostProcessor2 bean1 postProcessAfterInitialization...
    BeanPostProcessor1 bean1 postProcessAfterInitialization...
    set bean1...
    BeanPostProcessor2 bean2 postProcessBeforeInitialization...
    BeanPostProcessor1 bean2 postProcessBeforeInitialization...
    Bean2 afterPropertiesSet...
    Bean2 initMethod...
    BeanPostProcessor2 bean2 postProcessAfterInitialization...
    BeanPostProcessor1 bean2 postProcessAfterInitialization...
    Bean3 init...
    BeanPostProcessor2 bean3 postProcessBeforeInitialization...
    BeanPostProcessor1 bean3 postProcessBeforeInitialization...
    BeanPostProcessor2 bean3 postProcessAfterInitialization...
    BeanPostProcessor1 bean3 postProcessAfterInitialization...

    <bean id="bean2" class="com.cr.leftCycle.demo5.Bean2" init-method="initMethod">
        <property name="bean1" ref="bean1"></property>
    </bean>
    <bean id="bean1" class="com.cr.leftCycle.demo5.Bean1" init-method="initMethod"></bean>
    <bean id="bean3" class="com.cr.leftCycle.demo5.Bean3"></bean>
    <bean class="com.cr.leftCycle.demo5.BeanFactoryPostProcessor2"></bean>
    <bean class="com.cr.leftCycle.demo5.BeanFactoryPostProcessor1"></bean>
    <bean class="com.cr.leftCycle.demo5.BeanPostProcessor2"></bean>
    <bean class="com.cr.leftCycle.demo5.BeanPostProcessor1"></bean>
    -->

</beans>